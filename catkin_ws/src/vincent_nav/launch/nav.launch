<launch>


<!-- Run the map server -->


<!-- move_base -->

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">

    <rosparam file="$(find vincent_nav)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find vincent_nav)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find vincent_nav)/param/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find vincent_nav)/param/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find vincent_nav)/param/move_base_params.yaml" command="load" />

    <rosparam file="$(find vincent_nav)/param/base_local_planner_params.yaml" command="load" />


  </node>

</launch>
